@model BiteBazaarWeb.Models.ProductImage

@{
    ViewData["Title"] = "Create";
}

<link href="~/css/ViewSpecific/Admin.css" rel="stylesheet" />
<h1>Lägg till Bild</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group mb-4">
                <label asp-for="URL" class="control-label text-white">URL</label>
                <input asp-for="URL" class="form-control" />
                <span asp-validation-for="URL" class="text-danger"></span>
            </div>
           
            <div class="form-group">
                <input type="submit" value="Lägg Till" class="btn btn-egen-blue" />
                <a asp-controller="Products" asp-action="Index" class="btn btn-secondary">Tillbaka</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
